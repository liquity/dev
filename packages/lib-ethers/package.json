    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-plugin-tsdoc": "0.2.11",
    "ethers": "5.4.3",
    "fs-extra": "9.1.0",
    "hardhat": "2.6.4",
    "npm-run-all": "4.1.5",
    "ts-node": "9.1.1",
