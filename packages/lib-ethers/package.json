    "@types/ws": "7.4.4",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.18.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "chai-spies": "1.0.0",
