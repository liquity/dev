{
  "name": "@stabilio/subgraph",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "create-local": "graph create stabilio/stabilio --node http://127.0.0.1:8020",
    "prepare": "run-s prepare:*",
    "prepare:manifest": "node subgraph.yaml.js",
    "prepare:codegen": "graph codegen",
    "prepare-local": "run-s prepare-local:*",
    "prepare-local:manifest": "node subgraph.yaml.js dev",
    "prepare-local:codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy stabilio/stabilio --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy stabilio/stabilio --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "graph": "graph"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.0",
    "@graphprotocol/graph-ts": "^0.20.0",
    "npm-run-all": "^4.1.5"
  }
}
